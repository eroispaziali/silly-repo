<apex:page standardController="Contact" extensions="testContact" id="testcontactScoutPage">
<apex:form >
        <apex:pageBlock title="My Content" mode="edit">
            <apex:pageBlockButtons >
                <apex:commandButton action="{!save}" value="Save"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection title="My Content Section" columns="2">
                <apex:inputField value="{!contact.lastname}" id="lastname"/>
                <apex:inputField value="{!contact.firstname}"/>
                <apex:inputField value="{!contact.salutation}"/>
                <apex:inputField value="{!contact.mailingcity}" id="mailingCity"/>
                
                <apex:inputField value="{!contact.mailingcountry}" id="mailingCountry"/>
                <apex:inputField value="{!contact.mailingstate}" id="mailingState"/>
                <apex:inputField value="{!contact.mailingpostalcode}" id="mailingPostcode"/>
                <apex:inputField value="{!contact.mailingstreet}" id="mailingStreet"/>                
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>