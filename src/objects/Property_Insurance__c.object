    <validationRules>
        <fullName>Building_Replacement_Value_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISPICKVAL(Building__c,&apos;Tenant&apos;),  ISBLANK(Building_Replacement_Value__c),  (RecordType.DeveloperName  =&apos;Insurance_Charge&apos;), $User.IgnoreValidationRules__c=False),True,False)</errorConditionFormula>
        <errorDisplayField>Building_Replacement_Value__c</errorDisplayField>
        <errorMessage>“Building Replacement Value” field must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Insurance_Contents_Value_Required</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISPICKVAL(Contents__c,&quot;NA&quot;) || ISBLANK(TEXT(Contents__c)) ) 
&amp;&amp;
ISBLANK(Contents_Value__c)
&amp;&amp; 
$User.IgnoreValidationRules__c =False
&amp;&amp;
RecordType.DeveloperName=&apos;Insurance_Charge&apos;</errorConditionFormula>
        <errorDisplayField>Contents_Value__c</errorDisplayField>
        <errorMessage>&quot;Contents value&quot; field must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Insurance_Rent_Checked</fullName>
        <active>true</active>
        <description>Either Amount or Percentage has to be entered</description>
        <errorConditionFormula>$User.IgnoreValidationRules__c=False 
&amp;&amp; 
RecordType.DeveloperName=&apos;Insurance_Charge&apos; 
&amp;&amp; 
Insurance_Rent_Payable__c = TRUE 
&amp;&amp;  

     Text(Method_of_Payment_in__c)&lt;&gt;&quot;Paid as a part of Service Charge&quot;

&amp;&amp;
(
    ISBLANK( Insurance_Amount__c)  
          &amp;&amp;  
    ISBLANK(Insurance_Percentage__c )
)</errorConditionFormula>
        <errorDisplayField>Insurance_Amount__c</errorDisplayField>
        <errorMessage>Insurance Amount or Percentage to be filled if Insurance Rent is Payable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Insurance_Rent_Paid_Part_Of_Service_Char</fullName>
        <active>true</active>
        <description>Either Amount or Percentage has to be entered</description>
        <errorConditionFormula>$User.IgnoreValidationRules__c=False 
&amp;&amp; 
RecordType.DeveloperName=&apos;Insurance_Charge&apos; 
&amp;&amp; 
Insurance_Rent_Payable__c = TRUE 
&amp;&amp;  

     Text(Method_of_Payment_in__c)=&quot;Paid as a part of Service Charge&quot;

&amp;&amp;
(

   (
    NOT(ISBLANK( Insurance_Amount__c))  
          &amp;&amp;  
    NOT(ISBLANK(Insurance_Percentage__c ))
    )
      || 
   (
    NOT(ISBLANK( Insurance_Amount__c))  
          ||
    NOT(ISBLANK(Insurance_Percentage__c ))
   )
