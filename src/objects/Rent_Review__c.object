        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
ISPICKVAL( Rent_Review_Status__c , &quot;Completed&quot;)
&amp;&amp; 
ISBLANK(  Date_Agreed__c  )</errorConditionFormula>
        <errorDisplayField>Date_Agreed__c</errorDisplayField>
        <errorMessage>Date agreed must be enteredwhen  Rent Review Status is set to Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Agreed_must_be_null</fullName>
        <active>true</active>
        <description>can be filled only if the status is completed</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
(
ISPICKVAL( Rent_Review_Status__c , &quot;Not Started&quot;) || ISPICKVAL( Rent_Review_Status__c , &quot;In Progress&quot;)
) 
&amp;&amp; NOT(ISBLANK(  Date_Agreed__c  ))</errorConditionFormula>
        <errorDisplayField>Date_Agreed__c</errorDisplayField>
        <errorMessage>Can be filled only after the Rent Review Status is set to Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Request</fullName>
        <active>true</active>
        <description>This can be filled in only if the Rent review status is In progress</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
(
ISPICKVAL( Rent_Review_Status__c , &quot;In Progress&quot;)  || 
ISPICKVAL( Rent_Review_Status__c , &quot;Completed&quot;)
)
&amp;&amp; ISBLANK(  Date_Request__c  )</errorConditionFormula>
        <errorDisplayField>Date_Request__c</errorDisplayField>
        <errorMessage>Date Request from Landlord must be entered Rent Review Status is In Progress / Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Requested</fullName>
        <active>false</active>
        <description>can be filled only if the status is completed</description>
        <errorConditionFormula>(ISPICKVAL( Rent_Review_Status__c , &quot;Not Started&quot;) || ISPICKVAL( Rent_Review_Status__c , &quot;In Progress&quot;)) &amp;&amp; NOT(ISBLANK(  Date_Agreed__c  ))</errorConditionFormula>
        <errorDisplayField>Date_Agreed__c</errorDisplayField>
        <errorMessage>Can be filled only after the Rent Review Status is set to Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_cant_fill</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
(
ISPICKVAL( Review_Basis__c , &quot;Fixed Amount&quot;) || 
ISPICKVAL( Review_Basis__c , &quot;RPI&quot;) || 
ISPICKVAL( Review_Basis__c , &quot;CPI&quot;) || 
ISPICKVAL( Review_Basis__c , &quot;Open Market&quot;)
) 
&amp;&amp; NOT(ISBLANK( Percentage__c ))</errorConditionFormula>
        <errorDisplayField>Percentage__c</errorDisplayField>
        <errorMessage>Percentage can be filled in only if the Review basis is Fixed Percentage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_to_be_filled</fullName>
        <active>true</active>
        <description>can be filled only if Review Basis is Fixed Percentage</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
ISPICKVAL( Review_Basis__c , &quot;Fixed Percentage&quot;) &amp;&amp;
ISBLANK( Percentage__c )&amp;&amp;
( 
ISPICKVAL( Rent_Review_Status__c ,&quot;In progress&quot;) ||
ISPICKVAL( Rent_Review_Status__c ,&quot;Completed&quot;)
)</errorConditionFormula>
        <errorDisplayField>Percentage__c</errorDisplayField>
        <errorMessage>Percentage to be filled if the Review basis is Fixed Percentage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rent_Status_None_Not_Started</fullName>
        <active>true</active>
        <description>can be filled only if the status is completed</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
(ISBLANK(TEXT(Rent_Review_Status__c)) ||
ISPICKVAL(Rent_Review_Status__c,&quot;Not Started&quot; )) &amp;&amp;
(
NOT(ISBLANK(Date_Agreed__c)) ||  
NOT(ISBLANK(Date_Request__c)) ||  
NOT(ISBLANK(Amount_Requested__c)) ||  
NOT(ISBLANK(Date_Agreed__c)) ||  
NOT(ISBLANK(Amount_Agreed__c)) 
)</errorConditionFormula>
        <errorMessage>Rent Review Status = “None”, then all fields  should be blank
Date Request from Landlord,Amount Requested,Date Agreed ,Amount Agreed and Review Agreed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Review_Agreed</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
ISPICKVAL( Rent_Review_Status__c , &quot;Completed&quot;) 
&amp;&amp; 
Review_Agreed__c=False</errorConditionFormula>
        <errorDisplayField>Review_Agreed__c</errorDisplayField>
        <errorMessage>Review agreed must be entered when rent review status is completed.</errorMessage>
    </validationRules>
</CustomObject>
