    <validationRules>
        <fullName>Amount_Agreed_must_be_entered</fullName>
        <active>true</active>
        <description>can be filled only if the status is completed</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
ISPICKVAL( Rent_Review_Status__c , &quot;Completed&quot;) 
&amp;&amp;
ISBLANK(Amount_Agreed__c )</errorConditionFormula>
        <errorDisplayField>Amount_Agreed__c</errorDisplayField>
        <errorMessage>Amount Agreed must be entered when rent review status is completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_Agreed_must_be_null</fullName>
        <active>true</active>
        <description>can be filled only if the status is completed</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
(
ISPICKVAL( Rent_Review_Status__c , &quot;Not Started&quot;) || ISPICKVAL( Rent_Review_Status__c , &quot;In Progress&quot;)
) 
&amp;&amp; NOT(ISBLANK(Amount_Agreed__c ))</errorConditionFormula>
        <errorDisplayField>Amount_Agreed__c</errorDisplayField>
        <errorMessage>Amount Agreed must be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_Requested_Agreed</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
(
ISPICKVAL( Rent_Review_Status__c , &quot;Completed&quot;) || 
ISPICKVAL( Rent_Review_Status__c , &quot;In Progress&quot;)
)
&amp;&amp; 
ISBLANK(Amount_Requested__c)</errorConditionFormula>
        <errorDisplayField>Amount_Requested__c</errorDisplayField>
        <errorMessage>Amount Requested can be filled only after the Rent Review Status is Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_cant_fill</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
(
ISPICKVAL( Review_Basis__c , &quot;Fixed Percentage&quot;) || ISPICKVAL( Review_Basis__c , &quot;RPI&quot;) || 
ISPICKVAL( Review_Basis__c , &quot;CPI&quot;) || 
ISPICKVAL( Review_Basis__c , &quot;Open Market&quot;)
) 
&amp;&amp; 
NOT(ISBLANK(Fixed_Amount__c ))&amp;&amp;
(
ISBLANK(TEXT(Rent_Review_Status__c))  || 
ISPICKVAL(Rent_Review_Status__c,&quot;Not Started&quot;)
)</errorConditionFormula>
        <errorDisplayField>Fixed_Amount__c</errorDisplayField>
        <errorMessage>Amount can be filled in only if the Review basis is Fixed Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_to_be_filled</fullName>
        <active>true</active>
        <description>can be filled only if the Review basis is a fixed amount</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;
ISPICKVAL( Review_Basis__c , &quot;Fixed Amount&quot;) &amp;&amp;  
ISBLANK(  Fixed_Amount__c )&amp;&amp;
(ISPICKVAL( Rent_Review_Status__c ,&quot;Completed&quot;))</errorConditionFormula>
        <errorDisplayField>Fixed_Amount__c</errorDisplayField>
        <errorMessage>Amount to be filled if the Review basis is Fixed Amount</errorMessage>
    </validationRules>
    <validationRules>
