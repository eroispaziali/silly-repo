    <fields>
        <fullName>Information_Completed__c</fullName>
        <description>Cannot count faith or veteran or NI Trust as these are not gathered for all regions.</description>
        <externalId>false</externalId>
        <formula>CASE(( 
If(Isblank(NHS_Number__c),0,1) 
+If(Isblank(text(Ethnic_Group__c )),0,1) 
+If(Isblank(text(Ethnicitiy__c)),0,1) 
+If(Isblank(text(Living_arrangements__c)),0,1) 
+If(Isblank(text(Sexual_Status__c)),0,1) 
+If(Isblank(text(People_in_household__c)),0,1) 
+If(Isblank(text(Partnership_status__c)),0,1) 
+If(Isblank(text(Employment_Status__c)),0,1) 
