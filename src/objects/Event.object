  // The sforce API
  {!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
  
  var thisUrl = parent.location.href;


  // These calls return JSON objects. There is only one per call. 
  var res1 = sforce.connection.query(&quot;Select Id, Name from RecordType where IsActive=true and SobjectType=&apos;Task&apos; and DeveloperName =&apos;UI_Task&apos; limit 1&quot;);  

  var res2 = sforce.connection.query(&quot;Select Contact__c, Name from UI_Participant__c where id = &apos;&quot; + window.sfdcPage.getEntityId() + &quot;&apos; limit 1&quot;); 


     var rType = res1.get(&apos;records&apos;).Id; 
     var contactId = res2.get(&apos;records&apos;).Contact__c; 
     var contactName = res2.get(&apos;records&apos;).Name;
     
     var sUrl = 
           &apos;/00T/e?title=Call&amp;what_id=&apos; + 
            window.sfdcPage.getEntityId() +  
           &apos;&amp;followup=1&amp;retURL=&apos; + thisUrl + 
           &apos;&amp;RecordType=&apos;+ rType + 
           &apos;&amp;ent=Task&amp;tsk2=&apos; + contactName + 
