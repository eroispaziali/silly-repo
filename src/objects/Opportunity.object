    <validationRules>
        <fullName>Delivery_Date_on_Opportunity_Won</fullName>
        <active>true</active>
        <description>Delivery Date should be filled in, if the Opportunity Stage is Closed Won</description>
        <errorConditionFormula>ISBLANK( Delivery_Date__c ) &amp;&amp; ISPICKVAL(StageName ,&apos;Closed Won&apos;)</errorConditionFormula>
        <errorDisplayField>Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery Date should be filled in, if the Opportunity Stage is Closed Won</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_Name_Probability</fullName>
        <active>true</active>
        <description>Specific Stages on Opportunities must have specific Probabilities</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) &amp;&amp; ((RecordType.Name = &quot;Alzheimer&apos;s Trading Ltd Opportunity&quot;) &amp;&amp;
( (ISPICKVAL( StageName , &quot;Sales Qualified Lead&quot;) &amp;&amp; Probability &lt;&gt; 0.10) ||
  (ISPICKVAL( StageName , &quot;Product discussion&quot;) &amp;&amp; Probability &lt;&gt; 0.20) || 
  (ISPICKVAL( StageName , &quot;Quote&quot;) &amp;&amp; Probability &lt;&gt; 0.50) ||
  (ISPICKVAL( StageName , &quot;Quote review&quot;) &amp;&amp; Probability &lt;&gt; 0.70) ||
  (ISPICKVAL( StageName , &quot;Final negotiations&quot;) &amp;&amp; Probability &lt;&gt; 0.90) ||
  (ISPICKVAL( StageName , &quot;Closed Won&quot;) &amp;&amp; Probability &lt;&gt; 1) ||
  (ISPICKVAL( StageName , &quot;Closed Lost&quot;) &amp;&amp; Probability &lt;&gt; 0.00)))</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability can&apos;t be changed from its default value</errorMessage>
    </validationRules>
