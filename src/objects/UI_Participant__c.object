    <validationRules>
        <fullName>UI_Consent_Review_Date_Not_Passed</fullName>
        <active>true</active>
        <description>Ensures that Valid status cannot be set for Participation where Valid From is in the past and Valid To is in the Future.</description>
        <errorConditionFormula>AND ( 
     NOT( $User.IgnoreValidationRules__c ), 
     Or ( 
          ISPICKVAL(Status__c,&apos;Active&apos;),
          ISPICKVAL(Status__c,&apos;Pending Approval&apos;)
        ),    
     OR(
        ISBLANK(Valid_From_Date__c),
        Valid_From_Date__c &gt; TODAY(),
        Valid_From_Date__c &lt; TODAY()-180, 
        ISPICKVAL(Consent_Type__c,&quot;&quot;) 
     )
)</errorConditionFormula>
        <errorMessage>Participants must have a valid Consent to participate in Society activities. Please check Status is valid and the review date has not been exceeded.</errorMessage>
    </validationRules>
