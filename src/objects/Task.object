    <validationRules>
        <fullName>Completion_Date_Mandatory_on_Completion</fullName>
        <active>true</active>
        <description>Must be entered if Status set to &apos;Completed&apos;.</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) &amp;&amp; 
($RecordType.DeveloperName = &apos;CRS_Task&apos; &amp;&amp; ISBLANK( Completion_Date_Task__c) &amp;&amp;
  TEXT(Status ) = &quot;Completed&quot;)</errorConditionFormula>
        <errorDisplayField>Completion_Date_Task__c</errorDisplayField>
        <errorMessage>Completion Date is required for a Completed task.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>In_Out_required_unless_Face_to_Face</fullName>
        <active>true</active>
        <description>In/Out is required unless Format = Face to Face</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) &amp;&amp; 

  
   TEXT( In_Out__c )=&apos;&apos; 
&amp;&amp;  
   (
   NOT(
           ISPICKVAL(Format__c,&apos;Face to Face&apos;) 
                 ||                      
          ISPICKVAL(Format__c,&apos;Other&apos;)
       )
    )
&amp;&amp; 
  OR(
     $RecordType.DeveloperName = &apos;CRS_Task&apos;,
     $RecordType.DeveloperName = &apos;CRS_Information_Provision_Task&apos;,
     $RecordType.DeveloperName = &apos;Non_CRS_Advocacy_Task&apos;
  )</errorConditionFormula>
        <errorDisplayField>In_Out__c</errorDisplayField>
        <errorMessage>Value for In/Out is required unless Format is Face to Face or Other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Staff_Time_Required_on_Completion</fullName>
        <active>true</active>
        <description>Staff Time Required when status is complete</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp; 

    TEXT( Status)=&apos;Completed&apos; 
&amp;&amp; 
  ( 
      ISNULL(Staff_Time_Minutes__c )   
              || 
     (Staff_Time_Minutes__c &lt; 0)
    )
&amp;&amp; 
(
OR(
$RecordType.DeveloperName = &apos;CRS_Task&apos;, 
$RecordType.DeveloperName = &apos;CRS_Information_Provision_Task&apos;, 
$RecordType.DeveloperName = &apos;Non_CRS_Advocacy_Task&apos; 
)
)</errorConditionFormula>
        <errorDisplayField>Staff_Time_Minutes__c</errorDisplayField>
        <errorMessage>Staff time is required when task is completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Staff_Time_not_Required</fullName>
        <active>true</active>
        <description>Staff Time Required is not Required when Status is not Completed</description>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp; 
(
 TEXT( Status)!=&apos;Completed&apos; &amp;&amp; 
   ( 
   NOT(ISNULL(Staff_Time_Minutes__c )) 
             ||      
   NOT((Staff_Time_Minutes__c &lt; 0))
    )
)
&amp;&amp; 
(
OR(
$RecordType.DeveloperName = &apos;CRS_Task&apos;, 
$RecordType.DeveloperName = &apos;CRS_Information_Provision_Task&apos;, 
$RecordType.DeveloperName = &apos;Non_CRS_Advocacy_Task&apos; 
  )
)</errorConditionFormula>
        <errorDisplayField>Staff_Time_Minutes__c</errorDisplayField>
        <errorMessage>Staff time is not required when task is not completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_PostcodeSector_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( $User.IgnoreValidationRules__c ) 
&amp;&amp;  NOT(ISBLANK(Postcode_Sector__c))
&amp;&amp; $RecordType.DeveloperName = &apos;CRS_Information_Provision_Task&apos;   
&amp;&amp; NOT(REGEX(Postcode_Sector__c,&apos;[a-zA-Z]{1,2}[0-9][0-9a-zA-Z]?\\s[0-9]{1}&apos;))</errorConditionFormula>
        <errorDisplayField>Postcode_Sector__c</errorDisplayField>
        <errorMessage>Postcode sector must be in valid format.The postcode sector is made up of the postcode district, the single space, and the first character of the inward code. It is between five and six characters long (including the single space).</errorMessage>
